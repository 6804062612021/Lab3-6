#include <stdio.h>

int main() {
	int score,r;
	scanf("%d",&score);
	if(score>100){printf("error score");}
	else if(score >= 68) {	
			if(score >= 85){printf("A");}
			else if	(score>=75){printf("B");}
			else{r=78-score;printf("C, %d more points for B");}}
		else {
			if(score>=55){printf("D");}
			else if(score>=0){printf("F");}
			else{printf("error score");}}
}
